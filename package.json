{
  "name": "fsevents",
  "version": "1.2.4",
  "description": "Native Access to Mac OS-X FSEvents",
  "main": "fsevents.js",
  "dependencies": {
    "node-addon-api": "1.4.0",
    "node-pre-gyp": "^0.11.0"
  },
  "os": [
    "darwin"
  ],
  "engines": {
    "node": "^6.14.2 || ^8.12 || >=10"
  },
  "scripts": {
    "install": "node install",
    "prepublish": "if [ $(npm -v | head -c 1) -lt 3 ]; then exit 1; fi && npm dedupe",
    "test": "tap ./test",
    "node-pre-gyp": "node-pre-gyp"
  },
  "binary": {
    "module_name": "fse",
    "module_path": "./lib/binding/{configuration}/{platform}-{arch}-napi{napi_build_version}/",
    "remote_path": "./v{version}/",
    "package_name": "{module_name}-v{version}-napi{napi_build_version}-{platform}-{arch}.tar.gz",
    "host": "https://fsevents-binaries.s3-us-west-2.amazonaws.com",
    "napi_versions": [
      1
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/fsevents.git"
  },
  "keywords": [
    "fsevents",
    "mac"
  ],
  "author": "Philipp Dunkel <pip@pipobscure.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strongloop/fsevents/issues"
  },
  "bundledDependencies": [
    "node-pre-gyp"
  ],
  "homepage": "https://github.com/strongloop/fsevents",
  "devDependencies": {
    "tap": "^12.0.1"
  }
}
