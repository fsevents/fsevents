{
  "name": "fsevents",
  "version": "1.3.0",
  "description": "Native Access to Mac OS-X FSEvents",
  "main": "fsevents.js",
  "os": [
    "darwin"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "clean": "node-gyp clean && [ -f 'fsevents.bak.js' ] && mv -f fsevents.bak.js fsevents.js",
    "test": "tap ./test",
    "prepare": "node-gyp clean && node-gyp configure && node-gyp build",
    "install": "node installtest.js || ./fallback.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/fsevents.git"
  },
  "keywords": [
    "fsevents",
    "mac"
  ],
  "author": "Philipp Dunkel <pip@pipobscure.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strongloop/fsevents/issues"
  },
  "homepage": "https://github.com/strongloop/fsevents",
  "devDependencies": {
    "node-gyp": "^3.8.0",
    "tap": "~0.4.8"
  }
}
